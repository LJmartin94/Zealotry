<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.github.LJmartin94.zealotry">

	<uses-permission android:name="android.permission.INTERNET" />

	<application
		android:allowBackup="true"
		android:icon="@mipmap/ic_ziggurat2"
		android:label="@string/app_name"
		android:roundIcon="@mipmap/ic_launcher_round"
		android:supportsRtl="true"
		android:theme="@style/AppTheme">
		<activity android:name=".Evening_menu"></activity>
		<activity android:name=".Day_menu" />
		<activity android:name=".Morning_menu" /> <!-- <meta-data -->
		<!-- android:name="com.google.android.actions" -->
		<!-- android:resource="@xml/githubsearch" /> -->
		<activity
			android:name=".MainActivity"
			android:label="@string/app_name">
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity
			android:name=".WeatherForecast"
			android:label="@string/act_WeatherForecast"
			android:parentActivityName=".MainActivity" />
		<activity
			android:name=".GitHubSearch"
			android:label="@string/act_githubsearch"
			android:parentActivityName=".MainActivity" />
		<activity
			android:name=".NewActivity"
			android:label="@string/act_second"
			android:parentActivityName=".MainActivity" />
	</application>

</manifest>